<?xml version='1.0' encoding='UTF-8'?>
<applications>
  <application>
    <name>lighttpd</name>
    <logs>
      <log>
        <name>Access Log</name>
        <dir>lighttpd</dir>
        <filename>access.log</filename>
        <view_lotatelog>1</view_lotatelog>
        <time_format>%d/%b/%Y:%H:%M:%S</time_format>
        <time_pattern>[([0-9]{2}/[a-zA-Z]{3}/[0-9]{4}:[0-9]{2}:[0-9]{2}:[0-9]{2}</time_pattern>
        <use_regex>0</use_regex>
      </log>
      <log>
        <name>Error Log</name>
        <dir>lighttpd</dir>
        <filename>error.log</filename>
        <view_lotatelog>1</view_lotatelog>
        <time_format>%Y-%m-%d %H:%M:%S</time_format>
        <time_pattern>^([0-9]{4}-[0-9]{2}-[0-9]{2} [0-9]{2}:[0-9]{2}:[0-9]{2}): </time_pattern>
        <use_regex>0</use_regex>
      </log>
    </logs>
  </application>
  
